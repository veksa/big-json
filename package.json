{
  "name": "@veksa/big-json",
  "type": "module",
  "version": "1.0.1",
  "description": "A stream based implementation of JSON.parse and JSON.stringify for big data",
  "homepage": "https://github.com/veksa/big-json",
  "packageManager": "yarn@4.4.1",
  "author": {
    "name": "Alex Khizhnyi",
    "email": "mendler88@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:veksa/big-json.git"
  },
  "license": "MIT",
  "main": "./dist/cjs/bigJson.cjs",
  "types": "./dist/bigJson.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/bigJson.d.ts",
      "import": "./dist/bigJson.mjs",
      "default": "./dist/cjs/bigJson.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "keywords": [
    "big-json",
    "json",
    "streams",
    "parse",
    "stringify"
  ],
  "scripts": {
    "build": "npm run clean && tsup",
    "clean": "shx rm -rf dist",
    "compile": "tsc --noEmit",
    "lint": "eslint -c .eslintrc.js --quiet src/",
    "test": "jest"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@nabla/vite-plugin-eslint": "^2.0.5",
    "@stylistic/eslint-plugin": "^2.12.1",
    "@types/jest": "^29.5.14",
    "@types/jsonstream": "^0.8.33",
    "@types/through2": "^2.0.41",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-perfectionist": "^4.3.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^56.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "shx": "^0.3.4",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.0"
  },
  "dependencies": {
    "JSONStream": "^1.3.5",
    "json-stream-stringify": "^3.1.6",
    "through2": "^4.0.2"
  }
}
